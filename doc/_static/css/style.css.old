/* ============================== HEADER */
#HeaderWrapper {
    width:100%;
    height:155px;
    background-image:url(../images/header-background.jpg);
    background-repeat:repeat-x;
}

#Header {
    width:1000px;
    height:155px;
    margin:0 auto;
}

#HeaderTop {
    width:1000px;
    height:63px;
    padding:29px 0 0 0;
    overflow:hidden;
}

#Logo {
    width:243px;
    height:63px;
    float:left;
}

#Social {
    margin:0;
    padding:0;
    list-style-type:none;
    float:right;
}

#Social li {
    margin:0 0 0 7px;
    float:right;
}

#Menu1 {
    margin:0;
    padding:15px 0 0 0;
    list-style-type:none;
    float:none;
}

#Menu1 li {
    height:36px;
    margin:0 4px 0 0;
    border-left:1px #d3d3d3 solid;
    border-top:1px #d3d3d3 solid;
    border-right:1px #d3d3d3 solid;
    -webkit-border-radius: 3px 3px 0 0; /* Saf3-4, iOS 1-3.2, Android 1.6 */
    border-radius: 3px 3px 0 0; /* Opera 10.5, IE9, Saf5, Chrome, FF4+, iOS 4, Android 2.1+ */

    float:left;
}

#Menu1 li a {
    display:block;
    height:25px;
    padding:10px 10px 0 10px;
    border-left:1px #b7b7b7 solid;
    border-top:1px #b7b7b7 solid;
    border-right:1px #b7b7b7 solid;
    color:#0a65aa;
    font-size:12px;
    font-weight:bold;
    text-decoration:none;
    text-shadow: 1px 1px 3px #888; /* FF3.5+, Opera 9+, Saf1+, Chrome, IE10 */
    -webkit-border-radius: 3px 3px 0 0; /* Saf3-4, iOS 1-3.2, Android 1.6 */
    border-radius: 3px 3px 0 0; /* Opera 10.5, IE9, Saf5, Chrome, FF4+, iOS 4, Android 2.1+ */
    background-color: #e0dfdf;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#fafafa), to(#e0dfdf)); /* Saf4+, Chrome */
    background-image: -webkit-linear-gradient(top, #fafafa, #e0dfdf); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top, #fafafa, #e0dfdf); /* FF3.6+ */
    background-image: -o-linear-gradient(top, #fafafa, #e0dfdf); /* Opera 11.10+ */
    background-image: linear-gradient(to bottom, #fafafa, #e0dfdf);
}

#Menu1 li a.current,
#Menu1 li a.section,
#Menu1 li a:hover {
    color:#fff;
    background-color: #55b7ed;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#55b7ed), to(#165b81)); /* Saf4+, Chrome */
    background-image: -webkit-linear-gradient(top, #55b7ed, #165b81); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top, #55b7ed, #165b81); /* FF3.6+ */
    background-image: -o-linear-gradient(top, #55b7ed, #165b81); /* Opera 11.10+ */
    background-image: linear-gradient(to bottom, #55b7ed, #165b81);
}

#Menu1 li.homeButton a {
    margin:0;
    padding:0;
    width:41px;
    height:35px;
    background-image:url(../images/home-icon.jpg);
}

#Menu1 li.homeButton a.current,
#Menu1 li.homeButton a:hover {
    background-image:url(../images/home-icon-over.jpg);
}
/* ============================== END HEADER */





/* ============================== MAIN LAYOUT */
#Content {
